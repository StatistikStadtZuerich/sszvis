{
  "name": "sszvis",
  "version": "2.0.2",
  "description": "Statistik Stadt ZÃ¼rich Visualization Library",
  "keywords": [
    "d3",
    "visualization",
    "data visualization"
  ],
  "homepage": "https://statistikstadtzuerich.github.io/sszvis/",
  "repository": {
    "type": "git",
    "url": "https://github.com/statistikstadtzuerich/sszvis.git"
  },
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "d3": ">=4.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/standalone": "^7.8.3",
    "acorn": "^7.1.0",
    "babel-jest": "^24.9.0",
    "catalog": "3.6.0",
    "d3": "^4.12.0",
    "d3-dsv": "^1.0.8",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.3.0",
    "glob": "^7.1.6",
    "jest": "^24.9.0",
    "jest-image-snapshot": "^2.12.0",
    "jest-puppeteer": "^4.4.0",
    "jszip": "^3.1.5",
    "ndjson-cli": "^0.3.1",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "radium": "^0.26.0",
    "ramda": "^0.26.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rollup": "^1.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "topojson-client": "^3.0.0",
    "topojson-server": "^3.0.0",
    "topojson-simplify": "^3.0.1"
  },
  "files": [
    "src",
    "build"
  ],
  "main": "build/sszvis.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "unpkg": "build/sszvis.js",
  "jsdelivr": "build/sszvis.js",
  "scripts": {
    "start": "catalog start docs -p 8000",
    "build": "npm run build:lib && npm run build:topo && npm run build:docs",
    "build:lib": "rollup -c",
    "build:topo": "scripts/topo.sh",
    "build:docs": "catalog build docs -o build --public-url=./",
    "build:watch": "npm run build:lib -- --watch",
    "test": "npm run test:lint && npm run test:unit && npm run test:snapshot",
    "test:lint": "eslint src",
    "test:unit": "jest --testPathIgnorePatterns snapshot.test.js",
    "test:snapshot": "rm -rf test/__image_snapshots__/__diff_output__ && jest --testPathPattern snapshot.test.js",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run test && npm run build"
  }
}
